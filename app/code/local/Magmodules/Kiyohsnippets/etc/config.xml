<config>
	<modules>
		<Magmodules_Kiyohsnippets>
			<version>1.1.3</version>
		</Magmodules_Kiyohsnippets>
	</modules>
	<global>
		<helpers>
			<kiyohsnippets>
				<class>Magmodules_Kiyohsnippets_Helper</class>
			</kiyohsnippets>
		</helpers>
		<blocks>
			<kiyohsnippets>
				<class>Magmodules_Kiyohsnippets_Block</class>
			</kiyohsnippets>
		</blocks>
        <models>
            <kiyohsnippets>
				<class>Magmodules_Kiyohsnippets_Model</class>
            </kiyohsnippets>
        </models>
  		<resources>  
			<kiyohsnippets_setup>
				<setup>
					<module>Magmodules_Kiyohsnippets</module>
				</setup>
				<connection>
					<use>core_setup</use>
				</connection>
			</kiyohsnippets_setup> 
		</resources>	       				
	</global>
	<frontend>
		<layout>
			<updates>
				<magmodules_kiyohsnippets>
					<file>magmodules_kiyohsnippets.xml</file>
				</magmodules_kiyohsnippets>
			</updates>
		</layout>
        <translate>
            <modules>
                <magmodules_kiyohsnippets>
                    <files>
                        <default>Magmodules_Kiyohsnippets.csv</default>
                    </files>
                </magmodules_kiyohsnippets>
            </modules>
        </translate>		
	</frontend>
	<adminhtml>
        <translate>
            <modules>
                <magmodules_kiyohsnippets>
                    <files>
                        <default>Magmodules_Kiyohsnippets.csv</default>
                    </files>
                </magmodules_kiyohsnippets>
            </modules>
        </translate>
    </adminhtml>
	<default>
		<kiyohsnippets>
			<api>
				<enabled>0</enabled>
				<kiyoh_suffix>by KiyOh</kiyoh_suffix>
			</api>
		</kiyohsnippets>
	</default>
</config>